{
    "name": "younes-el-barnoussi/nova-translation",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "younes-el-barnoussi/laravel-translation": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Joedixon\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joedixon\\NovaTranslation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
