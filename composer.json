{
    "name": "joedixon/nova-translation",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "joedixon/laravel-translation": "^1.2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:eduPHP/laravel-translation.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Joedixon\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joedixon\\NovaTranslation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
